{
  "extends": "standard",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "comma-dangle": ["error", "always-multiline"],
    "no-multiple-empty-lines": ["error", {"max": 1}],
  }
}
