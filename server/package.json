{
  "name": "mock-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "cd node_modules; ln -sf ../src @",
    "dev": "NODE_ENV=development PORT=5000 && npm run lint && npm run kp && nodemon --exec babel-node src/index.js",
    "build": "rm -rf .lib && babel src -d .lib",
    "start": "NODE_ENV=production PORT=5000 pm2 start .lib/index.js -i 0 --name ms",
    "online": "npm run build && npm run start",
    "reload": "pm2 reload ms",
    "stop": "pm2 stop ms",
    "delete": "pm2 stop ms && pm2 delete ms",
    "lint": "eslint --ext .js src",
    "lint:fix": "eslint --ext .js src --fix",
    "kp": "lsof -n -iTCP:5000 -sTCP:LISTEN -n -l -P | grep 'LISTEN' | awk '{print $2}' | xargs kill -9"
  },
  "author": "Tivcrmn <zhangjitao0405@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "500px": "^0.5.1",
    "axios": "^0.16.2",
    "babel-preset-stage-3": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "colors": "^1.1.2",
    "config": "^1.26.2",
    "express": "^4.15.4",
    "ioredis": "^3.1.4",
    "log4js": "^4.1.0",
    "moment": "^2.18.1",
    "mongoose": "^4.11.9"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.11.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
